{
    "name": "config-doc-ui",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.10",
        "@fortawesome/free-brands-svg-icons": "^5.6.1",
        "@fortawesome/free-solid-svg-icons": "^5.6.1",
        "@fortawesome/react-fontawesome": "^0.1.3",
        "@martin_hotell/rex-tils": "^2.1.0",
        "@storybook/react": "^4.1.2",
        "@types/jest": "23.3.10",
        "@types/node": "10.12.15",
        "@types/react": "16.7.17",
        "@types/react-dom": "16.0.11",
        "@types/react-redux": "^6.0.11",
        "@types/react-test-renderer": "^16.0.3",
        "@types/storybook__addon-actions": "^3.4.1",
        "node-sass": "^4.11.0",
        "node-sass-chokidar": "^1.3.4",
        "npm-run-all": "^4.1.5",
        "react-redux": "^6.0.0",
        "react-scripts": "2.1.1",
        "react-scripts-ts": "^4.4.2",
        "redux": "^4.0.1",
        "redux-devtools-extension": "^2.13.7",
        "reselect": "^4.0.0",
        "tslint-immutable": "^5.0.1",
        "typescript": "3.2.2"
    },
    "scripts": {
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "lint": "tslint -t codeFrame -p ./tsconfig.json -c tslint.json",
        "build-css": "node-sass-chokidar ./src/index.scss ./src/index.css",
        "watch-css": "npm run build-css && node-sass-chokidar ./src/index.scss ./src/index.css --watch --recursive",
        "start-js": "react-scripts-ts start",
        "start": "npm-run-all -p watch-css start-js",
        "build-js": "react-scripts-ts build",
        "build": "npm-run-all build-css build-js",
        "run-storybook": "start-storybook -p 9001 -c .storybook",
        "storybook": "npm-run-all -p watch-css run-storybook"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "devDependencies": {
        "@babel/core": "^7.2.2",
        "@storybook/addon-info": "^4.1.2",
        "@types/storybook__react": "^4.0.0",
        "awesome-typescript-loader": "^5.2.1",
        "babel-loader": "^8.0.4",
        "react-docgen-typescript-webpack-plugin": "^1.1.0"
    }
}
