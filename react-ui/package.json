{
    "name": "react-ui",
    "version": "0.9.0-RC1",
    "private": true,
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.10",
        "@fortawesome/free-solid-svg-icons": "^5.6.1",
        "@fortawesome/react-fontawesome": "^0.1.3",
        "@martin_hotell/rex-tils": "^2.1.0",
        "@storybook/addon-info": "^4.1.2",
        "@storybook/react": "^4.1.2",
        "@types/history": "^4.7.2",
        "@types/react-redux": "^6.0.11",
        "@types/react-router": "^4.4.3",
        "@types/react-router-redux": "^5.0.17",
        "@types/react-test-renderer": "^16.0.3",
        "@types/storybook__addon-actions": "^3.4.1",
        "@types/storybook__react": "^4.0.0",
        "connected-react-router": "^6.0.0",
        "history": "^4.7.2",
        "node-sass": "^4.11.0",
        "node-sass-chokidar": "^1.3.4",
        "npm-run-all": "^4.1.5",
        "react": "^16.6.3",
        "react-docgen-typescript-webpack-plugin": "^1.1.0",
        "react-dom": "^16.6.3",
        "react-redux": "^6.0.0-beta.3",
        "react-router": "^4.4.0-beta.6",
        "react-router-dom": "^4.4.0-beta.6",
        "react-router-redux": "^5.0.0-alpha.9",
        "react-scripts-ts": "3.1.0",
        "react-test-renderer": "^16.6.3",
        "redux": "^4.0.1",
        "redux-devtools-extension": "^2.13.7",
        "reselect": "^4.0.0",
        "ts-loader": "^5.3.1",
        "tslint-immutable": "^5.0.1"
    },
    "scripts": {
        "test": "react-scripts-ts test --env=jsdom",
        "eject": "react-scripts-ts eject",
        "lint": "tslint -t codeFrame -p ./tsconfig.json -c tslint.json",
        "build-css": "node-sass-chokidar ./src/index.scss ./src/index.css",
        "watch-css": "npm run build-css && node-sass-chokidar ./src/index.scss ./src/index.css --watch --recursive",
        "start-js": "react-scripts-ts start",
        "start": "REACT_APP_VERSION= npm-run-all -p watch-css start-js",
        "build-js": "react-scripts-ts build",
        "build": "npm-run-all build-css build-js",
        "run-storybook": "start-storybook -p 9001 -c .storybook",
        "storybook": "npm-run-all -p watch-css run-storybook"
    },
    "devDependencies": {
        "@storybook/addon-actions": "^4.1.2",
        "@types/jest": "^23.3.10",
        "@types/node": "^10.12.15",
        "@types/react": "^16.7.17",
        "@types/react-dom": "^16.0.11",
        "typescript": "^3.2.2"
    }
}
